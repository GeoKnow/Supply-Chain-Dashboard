# Pages
GET        /                    controllers.Application.index
GET        /query               controllers.Application.query
GET        /map                 controllers.Application.map
GET        /delivery/:id        controllers.Application.delivery(id)

# REST API
GET        /sparql              controllers.API.sparql(query)
GET        /map/deliveries      controllers.API.deliveries(sourceId: String)

# Static resources
GET        /assets/*file        controllers.Assets.at(path="/public", file)
