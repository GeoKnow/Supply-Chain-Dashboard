  schema:name "Chair" ;
  sc:productPart [
    sc:product prod:ChairLeg01 ;
    sc:quantity "4"^^xsd:integer
  ] ;
  sc:productPart [
    sc:product prod:ChairBack01 ;
    sc:quantity  "1"^^xsd:integer
